<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        <!-- override -->
        <record id="sale.seq_sale_order" model="ir.sequence">
            <field name="name">Sales Order</field>
            <field name="code">sale.order</field>
            <field name="prefix">SQ</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="purchase.seq_purchase_order" model="ir.sequence">
            <field name="name">Purchase Order</field>
            <field name="code">purchase.order</field>
            <field name="prefix">PQ</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_type_sale_order_fwa" model="ir.sequence.type">
            <field name="name">Sale Order Falinwa</field>
            <field name="code">sale.order.fwa</field>
        </record>
        <record id="seq_sale_order_fwa" model="ir.sequence">
            <field name="name">Sales Order Falinwa</field>
            <field name="code">sale.order.fwa</field>
            <field name="prefix">SO</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_type_purchase_order_fwa" model="ir.sequence.type">
            <field name="name">Purchase Order Falinwa</field>
            <field name="code">purchase.order.fwa</field>
        </record>
        <record id="seq_purchase_order_fwa" model="ir.sequence">
            <field name="name">Purchase Order Falinwa</field>
            <field name="code">purchase.order.fwa</field>
            <field name="prefix">PO</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</openerp>